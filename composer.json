{
  "name": "b-tokman/validation",
  "type": "library",
  "description": "Simple standalone PHP validation trait. That can be user in any object",
  "license": "MIT",
  "keywords": [
    "php",
    "validation",
    "trait",
    "symfony validation",
    "standalone"
  ],
  "support": {
    "issues": "https://github.com/bTokman/validation/issues",
    "source": "https://github.com/bTokman/validation"
  },
  "authors": [
    {
      "name": "Bogdan Tokman",
      "email": "bogdantokman@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2.5",
    "symfony/validator": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "bTokman\\Validation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "bTokman\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit tests"
  }
}
